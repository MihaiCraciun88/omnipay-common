{
  "name": "bytic/omnipay-common",
  "type": "library",
  "license": "MIT",
  "description": "Common classes for drivers extending ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/ByTIC/omnipay-common",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Omnipay\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Omnipay\\Common\\Tests\\": "tests/src",
      "ByTIC\\Omnipay\\Common\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "ext-iconv": "*",
    "omnipay/common": "^2.3",
    "bytic/data-objects": "^1.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.0"
  }
}
